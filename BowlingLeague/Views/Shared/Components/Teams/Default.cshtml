@{
}

@model IEnumerable<int>
<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-team="1" asp-route-pageNum="1" class="btn btn-outline-primary">Home</a>

    @foreach (int t in Model)
    {
<a asp-controller="Home" asp-action="Index" asp-route-team="@t" asp-route-pageNum="1" class="btn @(ViewBag.SelectedType == t ? "btn-primary" : "btn-outline-primary")">@t</a>}
</div>  